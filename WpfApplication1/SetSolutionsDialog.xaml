<Window x:Class="WpfApplication1.SetSolutionsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        WindowStartupLocation="CenterOwner"
        Title="SetSolutionsDialog" Height="300" Width="300">
    <Grid>
        <DataGrid x:Name="xGrid" HorizontalAlignment="Left" Margin="60,114,0,0" VerticalAlignment="Top" Height="55" Width="165"/>
        <Button x:Name="OkBtn" Content="OK" IsDefault="True" HorizontalAlignment="Left" Margin="132,241,0,0" VerticalAlignment="Top" Width="75" Click="OkBtn_Click"/>
        <Button x:Name="CancelBtn" Content="Отмена" IsCancel="True" HorizontalAlignment="Left" Margin="212,241,0,0" VerticalAlignment="Top" Width="75"/>
    </Grid>
</Window>
